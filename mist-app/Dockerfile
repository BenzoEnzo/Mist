FROM openjdk:17

ARG GUI_CONNECT
ARG CRYPTO_MSG_CONNECT
ARG CRYPTO_KEY_CONNECT

ENV SERVER_PORT=8080

ENV GUI_CONNECT=${GUI_CONNECT}
ENV CRYPTO_MSG_CONNECT=${CRYPTO_MSG_CONNECT}
ENV CRYPTO_KEY_CONNECT=${CRYPTO_KEY_CONNECT}


COPY target/mist-0.0.1-SNAPSHOT.jar /app.jar


CMD ["java", "-jar", "/app.jar"]